{% extends 'index.njk' %}

{% block pageTitle %}
  {{ CONTENT_STRINGS.PAGE_TITLE }}
{% endblock %}

{% block content %}

  <div class="govuk-grid-row">
    <div class="govuk-grid-column-three-quarters-from-desktop">

      <h1 class="govuk-heading-xl" data-cy="heading">{{ CONTENT_STRINGS.PAGE_TITLE }}</h1>

      <a class="govuk-button" href="{{ SIGN_IN_URL }}" data-cy="continue-to-sign-in-link-button">{{ CONTENT_STRINGS.BUTTONS.CONTINUE_TO_SIGN_IN }}</a>
    </div>
  </div>

{% endblock %}
