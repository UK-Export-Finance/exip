{% extends 'index.njk' %}
{% from "govuk/components/back-link/macro.njk" import govukBackLink %}

{% block pageTitle %}
  {{ CONTENT_STRINGS.PAGE_TITLE }}
{% endblock %}

{% block content %}

  {{ govukBackLink({
    text: CONTENT_STRINGS.LINKS.BACK,
    href: BACK_LINK,
    attributes: {
      "data-cy": DATA_CY.BACK_LINK
    }
  }) }}

  <div class="govuk-grid-row">
    <div class="govuk-grid-column-three-quarters-from-desktop">
      <h1 class="govuk-heading-l" data-cy="{{ DATA_CY.HEADING }}">{{ CONTENT_STRINGS.PAGE_TITLE }}</h1>

      <p class="govuk-body" data-cy="processing">{{ CONTENT_STRINGS.PROCESSING }}</p>

      <p class="govuk-body" data-cy="further-information">{{ CONTENT_STRINGS.CONTACT_FURTHER_INFORMATION }}</p>

      <p class="govuk-body" data-cy="withdraw">{{ CONTENT_STRINGS.WITHDRAW }}</p>

      <p class="govuk-body" data-cy="contact-email">{{ CONTENT_STRINGS.CONTACT }}</p>
    </div>
  </div>


{% endblock %}
