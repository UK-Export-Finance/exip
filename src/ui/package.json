{
  "name": "exip-ui",
  "version": "1.0.0",
  "description": "Export Insurance Policies UI",
  "engineStrict": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "lint": "eslint ./server",
    "lint:fix": "eslint ./server --fix",
    "build": "rimraf ./public/js && rimraf ./public/css && webpack --mode production --config webpack.prod.config.js",
    "start": "ts-node server/index.ts",
    "dev": "webpack --config webpack.config.dev.js --watch",
    "test": "jest --notify --coverage --verbose --config=jest.config.js",
    "test-watch": "jest --watchAll --notify --coverage --verbose --config=jest.config.js --detectOpenHandles",
    "component-test": "jest --notify --config=jest.component-test.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/exip.git"
  },
  "author": "UK Export Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/exip/issues"
  },
  "homepage": "https://github.com/UK-Export-Finance/exip#readme",
  "dependencies": {
    "@types/compression": "^1.7.2",
    "@types/connect-flash": "^0.0.37",
    "@types/cookie-parser": "^1.4.3",
    "@types/csurf": "^1.11.2",
    "@types/express-session": "^1.17.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.9",
    "@types/nunjucks": "^3.2.1",
    "accessible-autocomplete": "^2.0.4",
    "axios": "^1.2.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "express-session": "^1.17.3",
    "govuk-frontend": "^4.4.0",
    "morgan": "1.10.0",
    "node-notifier": "^10.0.1",
    "nodemon": "2.0.20",
    "nunjucks": "3.2.3",
    "path": "0.12.7",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@types/jest": "^29.2.3",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-jest": "29.3.1",
    "babel-loader": "^9.1.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
