  "scripts": {
    "build": "NODE_ENV=dev keystone build --no-ui",
    "data-migration": "npx ts-node --transpile-only ./data-migration/version-2-to-version-3/index.ts",
    "dev": "keystone dev --no-ui",
    "documentation": "npx spectaql -t ./documentation/output ./documentation/config.yml",
