    "@keystone-6/auth": "^8.1.0",
    "@keystone-6/core": "^6.3.1",
    "@keystone-6/fields-document": "^9.1.1",
    "@types/express": "^5.0.0",
    "@types/overload-protection": "^1.2.4",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.7.9",
    "axios-mock-adapter": "^2.1.0",
    "date-fns": "^4.1.0",
