{
  "name": "@uk-export-finance/exip",
  "version": "1.4.0",
  "description": "Export Insurance Policies",
  "engineStrict": true,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "test": "jest --no-cache --runInBand",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "postinstall": "husky install",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "prettier --write src/ && eslint --ext .ts src/ --fix",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/exip.git"
  },
  "author": "UK Export Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/exip/issues"
  },
  "homepage": "https://github.com/UK-Export-Finance/exip#readme",
  "dependencies": {
    "cypress": "^10.4.0",
    "cypress-audit": "^1.1.0",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  }
}
