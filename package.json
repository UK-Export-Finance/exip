{
  "name": "@uk-export-finance/exip",
  "version": "1.15.2",
  "description": "Export Insurance Policies",
  "engineStrict": true,
  "engines": {
    "node": ">=18",
    "npm": ">=9.5.0"
  },
  "scripts": {
    "postinstall": "husky install",
    "lint": "npm i && npm i --legacy-peer-deps --prefix ./src/ui && eslint --ext .ts src/ui && npm i --prefix ./src/api && eslint --ext .ts src/api && eslint --ext .js e2e-tests",
    "lint:fix": "prettier --write src/ui  && eslint --ext .ts src/api --fix && eslint --ext .ts src/ui --fix &&  eslint --ext .js e2e-tests --fix",
    "install:all": "npm i && npm i --legacy-peer-deps --prefix ./src/ui && npm i --legacy-peer-deps --prefix ./src/api && npm i --legacy-peer-deps --prefix ./generate-exip-pricing-grid",
    "update:all": "npm update --save && npm update --save --legacy-peer-deps --prefix ./src/ui && npm update --save --prefix ./src/api && npm update --save --prefix ./generate-exip-pricing-grid",
    "audit:all": "npm audit && npm audit --prefix ./src/ui && npm audit --prefix ./src/api && npm audit --prefix ./generate-exip-pricing-grid"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UK-Export-Finance/exip.git"
  },
  "author": "UK Export Finance",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UK-Export-Finance/exip/issues"
  },
  "homepage": "https://github.com/UK-Export-Finance/exip#readme",
  "dependencies": {
    "@cypress-audit/lighthouse": "^1.4.2",
    "@cypress-audit/pa11y": "^1.4.2",
    "@keystone-6/auth": "^7.0.1",
    "@keystone-6/core": "^5.3.2",
    "@keystone-6/fields-document": "^8.0.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "cypress": "^12.16.0",
    "cypress-v10-preserve-cookie": "^1.2.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "graphql-tag": "^2.12.6",
    "node-notifier": "^10.0.1",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jest": "^29.6.0",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
